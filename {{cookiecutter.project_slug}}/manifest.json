{
  "name": "{{ cookiecutter.project_name }}",
  "description": "{{ cookiecutter.project_short_description.replace('\"', '\\\"') }}",
  "version": "{{ cookiecutter.version }}",
  "permissions": [
    "activeTab"
  ],
  "omnibox": { "keyword": "{{ cookiecutter.multicli_keyword }}" },
